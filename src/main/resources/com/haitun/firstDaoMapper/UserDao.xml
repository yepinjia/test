<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.firstDao.UserDao">
	
		<resultMap id="MapUser" type="com.example.domain.firstdomain.User">
	         <result property="id" column="id"/>
	         <result property="name" column="name"/>
       </resultMap>	
		<select id="findUser" parameterType="java.lang.Integer"  resultMap="MapUser">
			<![CDATA[
				select id , name from demo t where t.id < #{id}
			]]>
		</select>
</mapper>